{
    "name": "utop-actual",
    "exportedEnvVars": {
        "utop_version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "1.19.3"
        },
        "utop_enable": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "enable"
        },
        "FINDLIB": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib"
        },
        "version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "1.19.3"
        },
        "PATH": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/bin"
        },
        "utop_installed": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "true"
        }
    },
    "version": "1.19.3",
    "dependencies": {
        "@opam-alpha/react": ">= 1.0.0",
        "@opam-alpha/ocamlfind": ">= 1.5.6",
        "dependency-env": "https://github.com/npm-ml/dependency-env.git",
        "substs": "https://github.com/yunxing/substs.git",
        "@opam-alpha/ocamlbuild": "*",
        "@opam-alpha/lambda-term": ">= 1.9.0",
        "@opam-alpha/cppo": ">= 1.1.2",
        "@opam-alpha/lwt": "*",
        "nopam": "https://github.com/yunxing/nopam.git",
        "opam-installer-bin": "https://github.com/yunxing/opam-installer-bin.git"
    },
    "scripts": {
        "postinstall": "eval $(dependencyEnv) && nopam && ./configure --prefix $opam_prefix --${camlp4_enable:-disable}-camlp4 && make && make install && (opam-installer --prefix=$opam_prefix || true)"
    },
    "peerDependencies": {
        "@opam-alpha/ocaml": ">= 4.01.0"
    }
}
